{
  "name": "joy-security",
  "version": "1.2.0",
  "description": "Joy Security Desktop App",
  "main": "main.js",
  "author": {
    "name": "JoyLau",
    "email": "2587038142.liu@gmail.com"
  },
  "repository ": "https://github.com/joylau/joy-security.git",
  "license": "MIT",
  "keywords": [
    "JoyLau"
  ],
  "build": {
    "productName": "Joy Security",
    "appId": "cn.joylau.code.joy-security",
    "copyright": "Copyright Â© JoyLau. All rights reserved.",
    "compression": "store",
    "asar": true,
    "files": [
      "**/*",
      "build/",
      "!build/static/js/*.js.map",
      "!src/"
    ],
    "extraResources": [
      {
        "from": "./LICENSE",
        "to": "./../LICENSE.txt"
      }
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "icon": "./public/icons/win.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32",
            "x64"
          ]
        }
      ]
    },
    "mac": {
      "icon": "./public/icons/osx.icns",
      "extendInfo": {
        "URL types": [
          {
            "URL identifier": "Joy Security",
            "URL Schemes": [
              "joy-security"
            ]
          }
        ]
      }
    },
    "dmg": {
      "icon": "./public/icons/osx.icns"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./public/icons/win.ico",
      "uninstallerIcon": "./public/icons/win_uninstall.ico",
      "installerHeaderIcon": "./public/icons/win.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Joy Security",
      "license": "./LICENSE",
      "include": "./public/nsis/installer.nsh"
    }
  },
  "scripts": {
    "react-start": "node scripts/start.js",
    "eletron-start": "electron .",
    "react-build": "node scripts/build.js",
    "build": "node scripts/build.js && electron-builder -mw",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@svgr/webpack": "4.1.0",
    "antd": "^3.23.1",
    "autoprefixer": "latest",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "babel-plugin-import": "^1.12.1",
    "babel-plugin-named-asset-import": "^0.3.1",
    "babel-preset-react-app": "^7.0.2",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "css-loader": "1.0.0",
    "customize-cra": "^0.5.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "electron": "^11.5.0",
    "electron-builder": "^21.2.0",
    "electron-packager": "^14.0.5",
    "eslint": "5.12.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "2.0.0",
    "fs-extra": "^7.0.1",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.2",
    "jest-resolve": "23.6.0",
    "jest-watch-typeahead": "^0.2.1",
    "jquery": "^3.4.1",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "0.5.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.2.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.5.0",
    "postcss-safe-parser": "4.0.1",
    "react": "^16.9.0",
    "react-app-polyfill": "^0.2.2",
    "react-app-rewired": "^2.1.0",
    "react-dev-utils": "^8.0.0",
    "react-dom": "^16.9.0",
    "request": "^2.88.0",
    "request-progress": "^3.0.0",
    "resolve": "1.10.0",
    "sass-loader": "7.1.0",
    "semver": "^6.3.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.2.2",
    "url-loader": "1.1.2",
    "webpack": "4.28.3",
    "webpack-dev-server": "3.1.14",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  },
  "dependencies": {
    "electron-localshortcut": "^3.1.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "/Users/joylau/dev/idea-project/joy-security/joy-security-app/node_modules/jest-watch-typeahead/filename.js",
      "/Users/joylau/dev/idea-project/joy-security/joy-security-app/node_modules/jest-watch-typeahead/testname.js"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "homepage": "."
}
